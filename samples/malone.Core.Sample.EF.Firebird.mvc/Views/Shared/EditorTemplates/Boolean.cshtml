@model Nullable<bool>

@{
    bool b = false;
    if (Model != null)
    {
        b = Model.Value;

    }
    @Html.TextBox("", b,
     new { @class = "boolfield", type = "checkbox", data_toggle = "switch", data_inverse = "true", data_on_color = "primary", data_off_color = "default", data_size = "mini" })
}
