    <globalization enableClientBasedCulture="true" culture="es-ES" uiCulture="es-ES" />
    <authentication mode="None" />
    <compilation debug="true" targetFramework="4.7.1" />
    <httpRuntime targetFramework="4.5.2" />
    <httpModules />
