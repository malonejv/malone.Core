  <Target Name="GetAssemblyVersion">
    <GetAssemblyIdentity AssemblyFiles="$(TargetPath)">
      <Output TaskParameter="Assemblies" ItemName="ProjAssemblyInfo" />
    </GetAssemblyIdentity>
    <PropertyGroup>
      <Pattern>(\d+)\.(\d+)\.(\d+)</Pattern>
      <In>%(ProjAssemblyInfo.Version)</In>
      <OutVersion>$([System.Text.RegularExpressions.Regex]::Match($(In), $(Pattern)))</OutVersion>
    </PropertyGroup>
    <PropertyGroup>
      <Year>$([System.DateTime]::Now.ToString("yy"))</Year>
      <Month>$([System.DateTime]::Now.ToString("MM"))</Month>
      <Date>$([System.DateTime]::Now.ToString("dd"))</Date>
      <Time>$([System.DateTime]::Now.ToString("HHmm"))</Time>
    </PropertyGroup>
    <ItemGroup>
      <VersionNumber Include="$(OutVersion).$(Year)$(Month)$(Date)$(Time)" />
    </ItemGroup>
  </Target>
  <Target Name="GetSemVer">
    <GetAssemblyIdentity AssemblyFiles="$(TargetPath)">
      <Output TaskParameter="Assemblies" ItemName="ProjAssemblyInfo" />
    </GetAssemblyIdentity>
    <PropertyGroup>
      <Pattern>(\d+)\.(\d+)\.(\d+)</Pattern>
      <In>%(ProjAssemblyInfo.Version)</In>
      <OutVersion>$([System.Text.RegularExpressions.Regex]::Match($(In), $(Pattern)))</OutVersion>
    </PropertyGroup>
    <ItemGroup>
      <SemVer Include="$(OutVersion)" />
    </ItemGroup>
  </Target>
  <PropertyGroup>
